CREATE TABLE employeedata(
id INT PRIMARY KEY, 
name VARCHAR(25) NOT NULL, 
salary INT NOT NULL, 
role VARCHAR(15) NOT NULL);
INSERT INTO employeedata(id,name,salary,role) VALUES 
